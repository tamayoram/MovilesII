<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.contactosv2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".RegistroContactoActivity"
            android:label="@string/title_activity_registro_contacto"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".ContactosActivity"
            android:label="@string/title_activity_contactos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RegistroUsuarioActivity"
            android:label="@string/title_activity_registro_persona"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.Launcher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>